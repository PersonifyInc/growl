#include "Global-Common.xcconfig"


// deployment
COPY_PHASE_STRIP = NO
STRIP_INSTALLED_PRODUCT = NO


// linking
DEAD_CODE_STRIPPING = NO


// code generation
GCC_INLINES_ARE_PRIVATE_EXTERN = NO
GCC_OPTIMIZATION_LEVEL = 0


// language
OTHER_CFLAGS = $(COMMON_CFLAGS) -fcatch-undefined-behavior -ftrapv -fverbose-asm


// preprocessing
GCC_PREPROCESSOR_DEFINITIONS = $(COMMON_GCC_PREPROCESSOR_DEFINITIONS) DEBUG DEBUG_$(USER) FSCRIPT


// interface builder compiler
IBC_FLATTEN_NIBS = NO
